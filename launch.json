{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI (uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--host", "0.0.0.0",
        "--port", "9090"
      ],
      "cwd": "${workspaceFolder}/src",  // <--
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
